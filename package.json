    "humanize-duration": "^3.22.0",
    "snoostorm": "^1.3.0",
    "snoowrap": "^1.21.0"
  },
  "devDependencies": {
    "eslint-config-standard-typescript-prettier": "^2.0.1",
    "mdlog": "^1.0.3",
    "nodemon": "^2.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "eslintConfig": {
    "extends": "standard-typescript-prettier",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
